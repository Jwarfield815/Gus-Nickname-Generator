<!DOCTYPE html>
<html>
    <head>
        <link rel="stylesheet" href="/css/fonts.css" />
        <link rel="stylesheet" href="/css/styles.css" />
        <script src="/scripts/defNames.js"></script>
        <script src="/scripts/changeFont.js"></script>
    </head>
    
    <body>
        {{>header}}
        <div id="content">
            <div id="names" style="font-size: 2em"></div>
        </div>
        <script>
            const container = document.getElementById('names');
            container.textContent += '';
            allNames.forEach((name) => {
                container.innerHTML += `${name.name}<br>`;
            });
        </script>
    </body>
</html>